import java.util.Scanner;
public class Solution {
    
  public static void main (String[]args)
  {
	Scanner sc = new Scanner (System.in);
	char ch = sc.next ().charAt (0);
	  System.out.println (((ch >= 'a' && ch <= 'z')
						   || (ch >= 'A' && ch <= 'Z')) ? (ch >= 'A'
														   && ch <=
														   'Z' ? 1 : 0) : -1);
  }
}
