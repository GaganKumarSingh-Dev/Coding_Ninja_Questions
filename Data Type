public class Solution {
    public static int dataTypes(String type) {
        int ans = 0;
        if(type.equals("Integer") || type.equals("Float")){ans = 4;}
        else if(type.equals("Long") || type.equals("Double")){ans = 8;}
        else if(type.equals("Character")){ans = 1;}
        else{ans = -1;}
        return ans;
    }
}
